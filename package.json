{
  "name": "mcp-discord",
  "version": "1.4.0",
  "description": "Enterprise Model Context Protocol (MCP) server for Discord integration with 58+ advanced features including rate limiting, security monitoring, auto-moderation, analytics, and comprehensive enterprise management tools",
  "main": "build/index.js",
  "bin": {
    "mcp-discord": "build/index.js"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "build": "tsc",
    "build:clean": "rm -rf build && npm run build",
    "start": "node build/fast-start.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "dev:watch": "nodemon --exec \"npm run dev\"",
    "test-api": "node test-api.js",
    "validate-env": "node validate-env.js",
    "health-check": "node scripts/dev-utilities.js health",
    "dev-utils": "node scripts/dev-utilities.js",
    "benchmark": "node scripts/dev-utilities.js bench",
    "cleanup": "node scripts/dev-utilities.js cleanup",
    "report": "node scripts/dev-utilities.js report",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "docs:generate": "typedoc src/index.ts",
    "docker:build": "docker build -t mcp-discord:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mcp-discord:latest",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml up -d",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "discord",
    "claude",
    "ai",
    "chatbot",
    "automation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sachicali/discordmcp-suite.git"
  },
  "bugs": {
    "url": "https://github.com/sachicali/discordmcp-suite/issues"
  },
  "homepage": "https://github.com/sachicali/discordmcp-suite#readme",
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.50",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.25.13"
  }
}
